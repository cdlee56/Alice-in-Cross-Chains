pragma solidity ^0.4.18;
contract Custody Smart Contract {

    address public owner;
    address public owner2;

    mapping (uint => address) public profToActor;

    struct Action {
        address actor;
        uint value;
    }

    modifier onlyOwnedBy(address _requester, uint _barcodeId) {
    	require(profToActor[_barcodeId] == _requester);
      	_;
    }

    /* Initializes contract with initial supply tokens to the creator of the contract */
    function ChainofCustody() public approvable {
        owner = msg.sender;
    }

    function ClaimProf(uint _barcodeId) 
    approvable
    onlyOwnedBy(0x00, _barcodeId)
    public {
        require(msg.value == 2146291208791208);
    	profToActor[_barcodeId] = msg.sender;
    }
}
